{
  "expo": {
    "name": "DukaSmart",
    "slug": "alex-app",
    "version": "1.0.0",
    "orientation": "default",
    "icon": "./assets/images/icon.png",
    "scheme": "alexapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": false,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.alexapp.inventory"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.alexapp.inventory",
      "edgeToEdgeEnabled": false
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "updates": {
      "enabled": true,
      "checkAutomatically": "ON_LOAD",
      "fallbackToCacheTimeout": 0,
      "url": "https://u.expo.dev/5127bf51-96de-4409-92de-d0f0f4e88ed6"
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "5127bf51-96de-4409-92de-d0f0f4e88ed6"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    }
  }
}
